generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Url {
  id            String   @id @default(cuid())
  shortId       String   @unique
  originalUrl   String
  sessionId     String
  clicks        Int      @default(0)
  createdAt     DateTime @default(now())
  lastClickedAt DateTime?
}
